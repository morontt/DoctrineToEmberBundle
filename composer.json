{
    "name": "morontt/doctrine-to-ember",
    "type": "symfony-bundle",
    "description": "Conversion models Doctrine2 to the models Ember-Data",
    "keywords": [
      "orm",
      "doctrine2",
      "emberjs",
      "ember.js"
    ],
    "homepage": "https://github.com/morontt/DoctrineToEmberBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "morontt",
            "email": "morontt@gmail.com",
            "homepage": "http://morontt.info",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "symfony/framework-bundle": "~2.1"
    },
    "require-dev": {
        "doctrine/orm": "~2.2,>=2.2.3",
        "twig/twig": "~1.11"
    },
    "autoload": {
        "psr-0": {
            "Mtt\\Bundle\\DoctrineToEmberBundle": ""
        }
    },
    "target-dir": "Mtt/Bundle/DoctrineToEmberBundle"
}
